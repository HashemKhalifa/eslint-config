    "generate eslint with typescript",
    "ts"
  ],
  "license": "MIT",
  "packageConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HashemKhalifa/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/HashemKhalifa/eslint-config/issues"
  },
  "homepage": "https://github.com/HashemKhalifa/eslint-config#readme",
